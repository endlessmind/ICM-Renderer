<Window x:Class="ICM_Drawer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ICM_Drawer"
        mc:Ignorable="d"
        Title="MainWindow" Height="509.306" Width="1098.92" Background="#FFC9C9C9" UseLayoutRounding="True" Loaded="Window_Loaded">
    <Grid>
        <Button x:Name="btnRender" Content="Render" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Image x:Name="imgResult" Margin="0,50,0,0" VerticalAlignment="Center" Stretch="None" HorizontalAlignment="Center" SnapsToDevicePixels="True"/>
        <Button x:Name="btnSave" Content="Save" HorizontalAlignment="Center" Margin="0,0,0,10" VerticalAlignment="Bottom" Width="75" Click="btnSave_Click" />
        <Button x:Name="button1" Content="Insert A" HorizontalAlignment="Left" Margin="594,10,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click"/>
        <Label x:Name="lblCount" Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Button x:Name="btnAna" Content="Analyze data" HorizontalAlignment="Left" Margin="608,448,0,0" VerticalAlignment="Top" Width="75" Click="btnAna_Click"/>
    </Grid>
</Window>
